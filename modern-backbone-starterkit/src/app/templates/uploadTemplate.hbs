<form action="/article" enctype='multipart/form-data' method="post">
  <input type="hidden" id="s3-uploaded-url" name="s3-uploaded-url" value="/images/default.png">

  <label for="kmw-picture-input">Picture</label>
  <input accept="image/*" id="kmw-picture-input" name="picture" type="file">
  <br>

  <label for="kmw-headline-input">Headline</label>
  <input type="text" id="kmw-headline-input" name="headline">
  <br>

  <label for="kmw-subline-input">Subline</label>
  <input type="text" id="kmw-subline-input" name="subline">
  <br>

  <div class="kmw-recaptcha"></div>

  <input type="submit">
</form>

